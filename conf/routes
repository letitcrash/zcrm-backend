# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index

POST     /validation                controllers.HomeController.validationTest


GET     /setupdb                    controllers.SetupDBController.setupDataBase
GET     /token                      controllers.SignupController.token
GET     /gettoken                   controllers.SignupController.findTokenAsync
POST     /val                        controllers.SignupController.tokenAsync

GET  /getuser    controllers.SignupController.getUser

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
